set echo bottom center;
font echo 12 sansserif;
color echo pink;
echo &quot;*Note: With large biological molecules the hydrogen|atoms are commonly not shown.&quot;</script><menubar>false</menubar><toolbar>false</toolbar><resource>aquaporeMem.pdb.gz</resource><state>page7$0.jms</state><width>400.0</width>
