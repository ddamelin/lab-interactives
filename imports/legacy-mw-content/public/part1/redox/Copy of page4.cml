<content start="13" end="14">
<component><class>org.concord.modeler.PageButton</class>
<title>add stuff</title>
<opaque>false</opaque>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>0</model>
<script>define %w 1.9;
define %h 2.1;
define %y 1.05
define %x 6;
define %yStart 0;
loop 1;
	while (%y &lt; 24);
		add obstacle (%x, %y, %w, %h); 
		set %y %y+%h; 
	endwhile; 
	set %x %x+%w; 
	if (%yStart == 0);
		set %yStart 1;
		set %y 0.0;
	else;
		set %yStart 0;
		set %y 1.05;
	endif;
loop 6;</script>
<action>Execute MW script</action>
</component></content>

