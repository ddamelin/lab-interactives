<component><class>org.concord.modeler.PageSlider</class>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>1</model>
<value>100.0</value>
<minimum>100.0</minimum>
<maximum>3500.0</maximum>
<nstep>34</nstep>
<title>Atom Mass</title>
<width>80</width>
<height>250</height>
<orientation>1</orientation>
<opaque>false</opaque>
<tick>true</tick>
<labeltable>{value=&quot;3500.0&quot;, label=&quot;high&quot;}{value=&quot;100.0&quot;, label=&quot;low&quot;}</labeltable>
<script>set atom[0].rx 13.2;
set atom[0].ry 10.95756727
set atom[0].vy -138
set atom[1].rx 13.2;
set atom[1].ry 13.84243273;
set atom[1].vy 138;
set element[0].mass %val;
set %newVel sqrt((%startKE/%val)*2);
set atom[0].vy %newVel*-1;
set atom[1].vy %newVel;</script>
<change>Execute MW script</change>
</component></content>

<content start="18" end="19">
<component><class>org.concord.modeler.PageSlider</class>
