set atom[0].rx 13.2;
set atom[0].ry 10.95756727
set atom[0].vy -1*%newVal
set atom[1].rx 13.2;
set atom[1].ry 13.84243273;
set atom[1].vy %newVal;
set rbond[0].strength 0.002+(%val/1000);
</script>
<change>Execute MW script</change>
</component></content>

