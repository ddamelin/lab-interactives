     <void property="script">
      <string>set %radius %ellipse[0].width/2;
set %particleIn nearest(0,%ellipse[0].x + %radius, %ellipse[0].y + %radius, %radius);
if (%particleIn != -1);
	set atom[1].charge 0;
else;
	set atom[1].charge %alphaCharge;
endif;</string>
