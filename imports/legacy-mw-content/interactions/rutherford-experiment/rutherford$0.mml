     <void property="script">
      <string>if (%flowOn);
	set %y %height*rand();
	add atom 0 (1.1, %y);
	set %lastAtom %number_of_atoms - 1;
	set atom[%lastAtom].vx 3000;
	set atom[%lastAtom].charge %alphaCharge;
	select atom %lastAtom;
	traj on;
endif;</string>
     </void>
